call start
halt

.start
  push $st, @20
  add $bp, $st
  push $st, -2048
  push $vi(30), @16000
  push $vi(0), @0
  call loop
  ret

.draw
  push $st, 2048
  add $st, $st
  dup $st
  mov $vi(20), $st
  cast $vi(20), color
  mov $fb, $vi(20)
  push $st, @1
  add $vi(0), $st
  ret

.loop
  call draw
  cmp $vi(0), $vi(30)
  jmplt loop
  int 0
  push $vi(0), @0
  jmp loop
  ret
